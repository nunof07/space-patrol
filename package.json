{
    "name": "space-patrol",
    "version": "1.0.0",
    "description": "Shoot 'em up space game",
    "main": "index.js",
    "scripts": {
        "info": "npm-scripts-info",
        "build": "webpack --config webpack.prod.babel.js",
        "start": "webpack-dev-server --open --config webpack.dev.babel.js",
        "lint": "tslint --config tslint.json --project tsconfig.json --format stylish src/**/*.ts --exclude src/**/*.d.ts",
        "test": "mocha-webpack",
        "test:watch": "mocha-webpack --watch",
        "test:coverage": "rm -rf .nyc_output && rm -rf coverage && cross-env NODE_ENV=coverage nyc mocha-webpack \"src/**/!(index|*.d).ts\" \"test/**/*.ts\"",
        "release": "semantic-release",
        "ci:build": "cross-env NODE_ENV=travis npm run build",
        "postinstall": "rm -rf .cache-loader",
        "precommit": "pretty-quick --staged"
    },
    "scripts-info": {
        "info": "Display help information about available scripts",
        "build": "Build all deliverables",
        "start": "Start local server and watch for changes",
        "lint": "Analyze source code for errors",
        "test": "Run the tests",
        "test:watch": "Run the tests and watch for changes",
        "test:coverage": "Run the tests and report coverage",
        "release": "Publish release",
        "ci:build": "Build all deliverables (Travis)",
        "postinstall": "Run after npm install (cleanup Webpack's cache-loader folder)",
        "precommit": "Run before commit (format code)"
    },
    "repository": {
        "type": "git",
